{
    "name": "mintbridge/eloquent-auditing",
    "description": "An eloquent package for recording model events to a database",
    "keywords": ["eloquent", "audit", "auditing", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Dixon",
            "email": "paul.dixon@mintbridge.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "suggest": {
        "illuminate/support": "Required for Laravel support"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Mintbridge\\EloquentAuditing\\": "src/"
        }
    },
    "minimum-stability": "stable"
}